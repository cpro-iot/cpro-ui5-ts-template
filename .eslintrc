{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "prettier/prettier": ["error"]
    }
}
